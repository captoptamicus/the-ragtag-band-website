<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <link rel="stylesheet" href="../css/bootstrap.min.css">
     <link rel="stylesheet" href="../css/bootstrap.min.js">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha512-SfTiTlX6kk+qitfevl/7LibUOeJWlt9rbyDn92a1DqWOw9vWG2MFoays0sgObmWazO5BQPiFucnnEAjpAB+/Sw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
     <link rel="stylesheet" href="css/index.css">
      <link href="https://fonts.googleapis.com/css2?family=Lato:ital,wght@1,900&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Poppins&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha512-SfTiTlX6kk+qitfevl/7LibUOeJWlt9rbyDn92a1DqWOw9vWG2MFoays0sgObmWazO5BQPiFucnnEAjpAB+/Sw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    
    <link rel="shortcut icon" href="images/logo2.jpeg" type="image/x-icon">
    
    <script>
        var p = {
         onclick: function() {
        var rows = document.getElementById("mytabb").rows;
        for(var i = 0, ceiling = rows.length; i < ceiling; i++) {
            rows[i].onclick = function() {
             //alert(this.cells[1].innerHTML);
             document.getElementById("ks").value=this.cells[1].innerHTML;
             
            }
        }
    }
};

    </script>
    <title>Candidate records</title>
</head>
<body >
    <header id="headerrecord">
        <nav>
            <div class="container">
                <div class="text-center">
                    <h2 class="font-weight-bold" style="font-family: 'Lato', sans-serif; background-color:#06d6a0;">THE RAGTAG BAND ADMIN PANEL</h2>
                </div>
            </div>
        </nav>

    </header>
    <main class="site-main">
        <div class="container">
         <div class="row">
             <div class="col-lg-12 col-8">
           <div id="list">      
         <form action="/records" method="POST">
          <table class="table" id="mytabb">
              <thead class="thead-dark">
                  <tr>
                      <th>Name</th>
                      <th>Email</th>
                      <th>State</th>
                      <th>Year</th>
                      <th>Level</th>
                      <th>Result</th>
                      <th>Answer sheet</th>
                      <th>Delete</th>
                  </tr>
              </thead>
              <tbody >
               {{#each resultss}}

                  <tr id="nm" name="tyh">
                      <td>{{this.User_name}} </td>
                      <td>{{this.Email}} </td>
                      <td>{{this.State}}</td>
                      <td>{{this.Year}}</td>
                      <td>{{this.Level}}</td>
                      <td>{{this.calculation_res}}</td>
                      <td>
                          {{!-- <a href="/records" class="btn border-shadow delete btn-primary">
                              See Answer sheet
                           <span class="text-gradient"><i class="fa-2x fa fa-trash-o" aria-hidden="true"></i></span>
                          </a> --}}
                          <input  onclick="p.onclick()" type="submit" id="myemail" class="btn border-shadow delete btn-primary" value="See Answer Sheet">

                      </td>
                      {{!-- <td><a href=""><i class="fa-2x fa fa-trash text-dark" aria-hidden="true"></i></a> --}}
                      <td>
                          <a href="/{{this.id}}"><i name="ssada" class="fa-2x fa fa-trash text-dark" aria-hidden="true"></i></a>
                          
                      </td>
                  </tr>

{{/each}}
              </tbody>
          </table>
          <span ><input id="ks" type="email" name="uemm" style="border: 1px solid white;" readonly></span>
         </form>
         </div>
             </div>
         </div>
        </div>
    </main>
</body>
</html>